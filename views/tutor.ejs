    <div class="col-lg-12 col-md-8">
        <div class="card card-user" style="margin: 80px">
          <div class="card-header">
            <h5 class="card-title">Online Class Link </h5>
          </div>
          <div class="card-body">
            <form method="post" action="/tutor">
              <div class="row">
                <div class="col-md-5 pr-1">
                  <div class="form-group">
                    <label>Faculty's  link (disabled)</label>
                    <input type="text" class="form-control" disabled placeholder="Student Enrollment" value="To Students">
                  </div>
                </div>
               
               
              </div>
              <div class="row">
                <div class="col-lg-6 col-md-8">
                  <div class="form-group">
                    <label for="Name">Name of faculty</label>
                    <input type="text" class="form-control" placeholder="Name" name="name" id="ClientName" >
                  </div>
                </div>
                <div class="col-lg-6 col-md-8">
                  <div class="form-group">
                    <label>Subject</label>
                    <input type="text" name="subject" class="form-control" placeholder="Subject" >
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6 col-md-8">
                  <div class="form-group">
                    <label for="Name">For Class</label>
                    <input type="text" class="form-control" placeholder="class" name="class" id="ClientName" >
                  </div>
                </div>
                <div class="col-lg-6 col-md-8">
                  <div class="form-group">
                    <label>Time of class (24 hour format)</label>
                    <input type="time" name="time" class="form-control" placeholder="time" >
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6 col-md-8">
                  <div class="form-group">
                    <label>Link</label>
                    <input type="url" name="url" class="form-control" placeholder="Link" >
                  </div>
                </div>
              </div>
             
              
              <div class="row">
                <div class="update ml-auto mr-auto">
                  <button  type="submit" class="btn btn-warning btn-round" id="ClientButton"  onclick="addButton()">Add LInk For Class</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>

 




